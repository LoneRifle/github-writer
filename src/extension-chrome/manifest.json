{
	"manifest_version": 2,
	"name": "GitHub RTE",
	"description": "Rich-text editing in GitHub. Powered by CKEditor 5.",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"https://github.com/fredck/github-rte-playground/issues/*"
			],
			"js": [
				"index.js"
			],
			"css": [
				"github-rte.css"
			],
			"run_at": "document_end"
		}
	]
}
